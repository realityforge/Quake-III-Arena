# unzip is (probably) legacy code to unzip which should switch to zlib
# It is also not "third_party" as such as it uses Z_Malloc and Z_Free
# However if we switch to zlib and pass in allocators/deallocators then we should be fine
cc_library(
    name = "unzip",
    srcs = [
        "ioapi.c",
        "ioapi.h",
        "unzip.c",
    ],
    hdrs = [
        "unzip.h",
    ],
    includes = ["."],
    visibility = [
        "//code/client:__pkg__",
        "//code/qcommon:__pkg__",
        "//code/server:__pkg__",
    ],
    deps = [
        "//code/qcommon",
        "//code/qcommon:shared_api",
        "@zlib",
    ],
)
